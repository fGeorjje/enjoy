{
  "name": "enjoy",
  "version": "1.1.0",
  "main": "build/server/server.js",
  "scripts": {
    "lint": "ts-standard src/**/*.ts --fix",
    "clean": "trash build",
    "build": "parcel build"
  },
  "author": "Paul Schwandes",
  "description": "",
  "targets": {
    "discord-inject": {
      "source": "src/discord-inject/**/*.ts",
      "distDir": "build/discord-inject",
      "context": "browser",
      "includeNodeModules": true,
      "engines": {
        "browsers": "Chrome 134"
      }
    },
    "obs-frontend": {
      "source": "src/obs-frontend/**/*.html",
      "distDir": "build/obs-frontend",
      "publicUrl": ".",
      "context": "browser",
      "includeNodeModules": true,
      "engines": {
        "browsers": "Chrome 127"
      }
    },
    "server": {
      "source": "src/server/server.ts",
      "distDir": "build/server",
      "context": "node",
      "outputFormat": "commonjs",
      "engines": {
        "node": "= 20"
      }
    }
  },
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "@types/textfit": "^2.4.5",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "fast-deep-equal": "^3.1.3",
    "obs-websocket-js": "^5.0.6",
    "textfit": "^2.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/ws": "^8.18.1",
    "parcel": "^2.15.4",
    "trash-cli": "^6.0.0",
    "ts-standard": "^12.0.2",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2"
  }
}
